<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<sheets>
	<sDEF>
		<ROOT>
			<TCEforms>
			<sheetTitle>RDFa</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
			<switchableControllerActions>
				<TCEforms>
					<label>Elemento</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
								<numIndex index="0">Comments</numIndex>
								<numIndex index="1">Content->likeButton</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</switchableControllerActions>
			<settings.templateFile>
				<TCEforms>
					<label>Template File (optional)</label>
					<config>
						<type>input</type>
						<wizards type="array">
						<_PADDING type="integer">2</_PADDING>
							<link type="array">
								<type>popup</type>
								<title>Link</title>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=wizard&amp;act=file</script>
								<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								<params type="array">
									<blindLinkOptions>page,url,mail,spec,folder</blindLinkOptions>
									<allowedExtensions>html</allowedExtensions>
								</params>

							</link>
						</wizards>
					</config>
				</TCEforms>
			</settings.templateFile>
			<settings.name>
				<TCEforms>
					<label>name</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.name>
			<settings.nickname>
				<TCEforms>
					<label>nickname</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.nickname>
			<settings.photo>
				<TCEforms>
					<label>photo</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.photo>
			<settings.role>
				<TCEforms>
					<label>role</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.role>
			<settings.url>
				<TCEforms>
					<label>url</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.url>
			<settings.org>
				<TCEforms>
					<label>affiliation</label>
					<config>
						<type>input</type>
						<eval>trim,nospace</eval>
					</config>
				</TCEforms>
			</settings.org>
			</el>
		</ROOT>
	</sDEF>
	</sheets>
</T3DataStructure>
